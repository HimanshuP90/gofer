{
  "name": "gofer",
  "version": "2.4.2",
  "description": "A general purpose service client library for node.js",
  "main": "lib/gofer.js",
  "scripts": {
    "build": "npub prep && coffee -cbo lib src",
    "prepublish": "rm -rf lib && npm run build",
    "pretest": "npm run build",
    "test": "mocha",
    "posttest": "npub verify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/groupon/gofer"
  },
  "keywords": [
    "gofer",
    "service",
    "http"
  ],
  "license": "BSD-2-Clause",
  "author": {
    "name": "Jan Krems",
    "email": "jan.krems@groupon.com>"
  },
  "contributors": [
    {
      "name": "Sean McCullough",
      "email": "mccullough@groupon.com"
    },
    {
      "name": "Sean Massa",
      "email": "smassa@groupon.com"
    },
    {
      "name": "Kofi Appiah",
      "email": "kofi@groupon.com"
    }
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "license": {
      "exclude": [
        "lib",
        "test"
      ]
    }
  },
  "dependencies": {
    "bluebird": "^2.9.13",
    "debug": "^2.1.2",
    "hrduration": "^1.0.0",
    "lodash": "^3.0.0",
    "node-uuid": "^1.4.1",
    "request": "2.57.0"
  },
  "devDependencies": {
    "assertive": "^1.4.0",
    "bondjs": "^1.1.1",
    "coffee-script": "1.9.0",
    "deepmerge": "^0.2.7",
    "express": "^4.1.1",
